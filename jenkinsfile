pipeline {
   agent {
      docker {
        image 'python:3'
        label 'my-build-agent'
      }
   }   
   stages {
      stage (version){
         steps {
	   sh "python3 --version"
	   }
      }
      stage ('Run puthon script'){
	    steps {
		  script {
		    //Run python script and save output in a file
			sh "python3 script1.py"
		  }
	    }
	  }
    }
}	
