pipeline {
   agent {
      docker {
        image 'python:3'
        label 'my-build-agent'
      }
   }   
   stages {
      stage ('Run puthon script'){
	    steps {
		  script {
		    //Run python script and save output in a file
			sh "python script1.py"
		  }
	    }
	  }
    }
}	
